package ch01;


import java.util.Iterator;
import java.util.Scanner;

public class test33 {

    public static void main(String[] args) {
       System.out.println("자릿수를 알고 싶은 것을 입력");
        Scanner in =new Scanner(System.in);
        int count=0;
        int num;
        int input = in.nextInt();
        
  
        
        while(input>0) {
            input/=30;
            count++;
        
        
           
        
        }
        
        System.out.println(count+"는 자릿수입니다.");
      
  
        
    }

   
      
   }

